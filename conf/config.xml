<?xml version="1.0" encoding="UTF-8"?>

<configurations>
    <import>
        <from>
            <corpusDir memo="the directory that stores the txt files"
                       doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigDirectoryChooser">data/input
            </corpusDir>
            <fileFilter
                    memo="filter files by extension names (separate by whitespace). If not filter defined, all files will be read"/>
        </from>
        <ehost>
            <projectDir memo="the directory that hosts the eHOST project"
                        doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigDirectoryChooser">
                /media/brokenjade/Data/Dropbox/JLS_HF_AnnotationTask/JLS_Ann_Training/Training_1
            </projectDir>
            <overWriteAnnotatorName
                    memo="if a name is set, all the annotations' annotator will be overwritten by this name"/>
        </ehost>
        <executes>
            <ImportData memo="Import corpus to database">edu.utah.bmi.simple.gui.task.Import</ImportData>
            <ViewInDatabase memo="View the imported files in database">edu.utah.bmi.simple.gui.task.ViewImportDB
            </ViewInDatabase>
            <ImportEhost memo="Import ehost annotations into the output table">
                edu.utah.bmi.simple.gui.task.ImportEhost
            </ImportEhost>
        </executes>
    </import>
    <easycie>
        <pipeLineSetting>
            <ruleFile memo="token based rules" doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigFileChooser"
                      openable="true">conf/concept_rule.xlsx
            </ruleFile>
            <fastNERCaseSensitive memo="whether the rule above for the FastNER is case sensitive">false
            </fastNERCaseSensitive>
            <cRuleFile memo="character based rules" doubleClick=" edu.utah.bmi.simple.gui.doubleclick.ConfigFileChooser"
                       openable="true">conf/fever_crule.xlsx
            </cRuleFile>
            <featureInfEvidenceConcept>edu.utah.bmi.type.system.Concept</featureInfEvidenceConcept>
            <featureInfRule>NegatedConcept:Negation,negated|PastConcept:Negation,affirmed&amp;Temporality,historical&amp;Experiencer,patient|NonPatientConcept:Negation,affirmed&amp;Experiencer,nonpatient</featureInfRule>
            <docInfRule>Pres_DOC:Concept|PastPres_DOC:PastConcept</docInfRule>
            <docInfDefault>NoPres_DOC</docInfDefault>
            <report memo="whether report the processing details at the end">false</report>
        </pipeLineSetting>
        <annotators>
            <current memo="Mark current annotations with annotator: ">v3</current>
        </annotators>
        <executes>
            <RunEasyCIE memo="Run Preannotator using current rules">edu.utah.bmi.simple.gui.task.RunEasyCIE</RunEasyCIE>
            <ViewOutputInDB memo="View the output in database">edu.utah.bmi.simple.gui.task.ViewOutputDB
            </ViewOutputInDB>
        </executes>
    </easycie>
    <compare>
        <compare>
            <sourceAnnotator memo="Compare annotations from annotator: ">v2</sourceAnnotator>
            <referenceAnnotator memo="Compare annotations against annotator: ">v1</referenceAnnotator>
            <strictCompare>true</strictCompare>
            <typeFilter memo="limit the comparison to a certain type"/>
            <categoryFilter memo="limit the comparison to a certain category"/>
        </compare>
        <executes>
            <Compare memo="Compare the difference  two annotators">edu.utah.bmi.simple.gui.task.CompareTask</Compare>
            <ViewComparedResults memo="View the output in database">edu.utah.bmi.simple.gui.task.ViewDiffDB
            </ViewComparedResults>
        </executes>
    </compare>
    <export>
        <format>
            <ehost memo="the directory to save the exported ehost files"
                   doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigDirectoryChooser">data/output/ehost
            </ehost>
            <brat memo="the directory to save the exported brat files"
                  doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigDirectoryChooser">data/output/brat
            </brat>
            <uima memo="the directory to save the exported uima xmi files"
                  doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigDirectoryChooser">data/output/xmi
            </uima>
            <uimaTypes memo="If specified, then only these types will be displayed(separate by comma)">Sentence,
                SentenceOdd
            </uimaTypes>
        </format>
        <executes>
            <ExportEhost memo="Run Preannotator and export the annotation to eHOST format">
                edu.utah.bmi.simple.gui.task.RunEasyCIE ehost
            </ExportEhost>
            <ExportBrat memo="Run Preannotator and export the annotation to brat format">
                edu.utah.bmi.simple.gui.task.RunEasyCIE brat
            </ExportBrat>
            <ExportXMI memo="Run Preannotator and export the annotation to UIMA XMI format">
                edu.utah.bmi.simple.gui.task.RunEasyCIE xmi
            </ExportXMI>
            <View memo="View exported UIMA annotation format">edu.utah.bmi.simple.gui.task.Viewer</View>
        </executes>
    </export>
    <otherPiplines>
        <PreAnnotatorCPE>
            <cpeDescriptor memo="CPE descriptor location" doubleClick="edu.utah.bmi.simple.gui.doubleclick.RunCpmFrame">
                desc/cpe/Preannotator.xml
            </cpeDescriptor>
            <report memo="Whether to display the processing time and results at the end.">false</report>
            <SQL_Text_Reader.SQLFile memo="the db file(or configuration file) used to saved the outputs"
                                     doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigFileChooser">
                /home/brokenjade/IdeaProjects/PreannnotatorGUI/data/fever.sqlite
            </SQL_Text_Reader.SQLFile>
            <SQL_Writer_CasConsumer.Annotator memo="use this string to differentiate different running results">c1
            </SQL_Writer_CasConsumer.Annotator>
            <customizedTypes
                    memo="external types that are not in the current type system, and need to be automatically generated">
                Digit, Float
            </customizedTypes>
        </PreAnnotatorCPE>
        <testPipeline>
            <cpeDescriptor memo="CPE descriptor location" doubleClick="edu.utah.bmi.simple.gui.doubleclick.RunCpmFrame">
                desc/cpe/testCPE.xml
            </cpeDescriptor>
            <report memo="Whether to display the processing time and results at the end.">true</report>
            <SQL_Text_Reader.SQLFile doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigFileChooser">
                /home/brokenjade/IdeaProjects/PreannnotatorGUI/data/fever.sqlite
            </SQL_Text_Reader.SQLFile>
            <SQL_Writer_CasConsumer.Annotator>t1</SQL_Writer_CasConsumer.Annotator>
            <customizedTypes
                    memo="external types that are not in the current type system, and need to be automatically generated">
                edu.utah.bmi.type.system.MyType1,edu.utah.bmi.type.system.MyType2
            </customizedTypes>
        </testPipeline>
        <executes>
            <PreannotatorCPE memo="demo how to set up customized CPE pipeline">
                edu.utah.bmi.simple.gui.task.GenericAdaptableCPERunner PreAnnotatorCPE
            </PreannotatorCPE>
            <testPipeline memo="demo how to set up customized CPE pipeline">
                edu.utah.bmi.simple.gui.task.GenericAdaptableCPERunner testPipeline
            </testPipeline>
            <OSListCommand memo="demo how to execute OS command">edu.utah.bmi.simple.gui.task.ExecuteOsCommand ls
            </OSListCommand>
            <OSMsgCommand memo="demo how to execute OS command with progress &amp; message show">
                edu.utah.bmi.simple.gui.task.ExecuteOsCommand ./test.sh
            </OSMsgCommand>
        </executes>
    </otherPiplines>
    <settings>
        <import>
            <dbFile memo="the db file(or configuration file) used to saved the txt corpus"
                    doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigFileChooser">data/pneumonia.sqlite
            </dbFile>
            <table memo="the table used to saved the txt corpus">corpus</table>
            <datasetId memo="data set id (Default is '0'. Use to differentiate different dataset in the same table.)">0</datasetId>
            <overwrite memo="if the table exist, whether overwrite it">true</overwrite>
        </import>
        <output>
            <dbFile memo="the db file(or configuration file) used to saved the outputs"
                    doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigFileChooser">data/pneumonia_output.sqlite
            </dbFile>
            <table memo="the table used to saved the annotations">output</table>
            <compareTable memo="the table used to saved the annotations">diff</compareTable>
        </output>
        <nlpComponents>
            <rush memo="RuSH sentence segmenter configuration file"
                  doubleClick="edu.utah.bmi.simple.gui.doubleclick.ConfigFileChooser">conf/rush.xlsx
            </rush>
        </nlpComponents>
        <viewer>
            <preTag>&lt;html&gt;&lt;body style="margin: 0; padding: 0"&gt; &lt;div id="mydiv"&gt;</preTag>
            <postTag>&lt;/div&gt; &lt;/body&gt;&lt;/html&gt;</postTag>
            <highlighter_begin
                    memo="Set default color to FFFFF, then the viewer will pick one from the color pool below.">&lt;span
                style="background-color: #FFFFFF"&gt;
            </highlighter_begin>
            <highlighter_end>&lt;/span&gt;</highlighter_end>
            <snippet_length>90</snippet_length>
            <random_pick_color memo="whether randomly pick a color from the color pool below or pick in order">true
            </random_pick_color>
            <color_pool>
                e32636|efdecd|ffbf00|00ffff|9966cc|ed872d|a3c1ad|a4c639|8db600|89cff0|fbceb1|7fffd4|e9d66b|87a96b|ff9966|21abcd|deb887|848482|fdee00|f4c2c2|ff2052|a1caf1|007fff|b2beb5|5d8aa8
            </color_pool>
        </viewer>
    </settings>
</configurations>
